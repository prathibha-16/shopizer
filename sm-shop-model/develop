pipeline {
     agent {label 'nvm'} 
     triggers {
        cron('30 17 * * *')
     }
     stages {
         stage('clone') { 
             steps {
                 git url:'https://github.com/prathibha-16/shopizer.git' , branch : 'release' 
             }
         }
         stage('install') { 
             steps {
                sh'mvn package' 
             }
         }
     }
 }
